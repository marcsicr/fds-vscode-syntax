{
  "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
  "name": "FDS",
  "patterns": [
    {
      "include": "#keywords"
    },
    {
      "include": "#strings"
    },
    {
      "include": "#type-name"
    },
    {
      "include": "#comments"
    }
  ],
  "repository": {
    "keywords": {
      "patterns": [
        {
          "name": "keyword.control.fds",
          "match": "\\b(if|else|do|endif|while|done|for|gosub|return)\\b"
        }
      ]
    },
    "strings": {
      "name": "string.quoted.double.fds",
      "begin": "\"",
      "end": "\"",
      "patterns": [
        {
          "name": "constant.character.escape.fds",
          "match": "\\\\."
        }
      ]
    },
    "comments": {
      "patterns": [
        {
          "name": "comment.line.double-slash.fds",
          "match": "^[.]+.*$"
        },
        {
          "name": "comment.line.double-slash.fds",
          "begin": "/[*]+",
          "end": "$"
        }
      ]
    }
  },
  "scopeName": "source.f"
}
