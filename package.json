{
  "name": "fds",
  "displayName": "FDS",
  "description": "FDS Syntax Colors",
  "icon": "images/icon.png",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/marcsicr/fds-vscode-syntax.git"
  },
  "publisher": "msimon@inducontrol.com",
  "engines": {
    "vscode": "^1.71.0"
  },
  "categories": [
    "Programming Languages",
    "Themes"
  ],
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": "kdocs.fds",
        "settings": {
          "foreground": "#33f"
        }
      }
    ]
  },
  "contributes": {
    "languages": [
      {
        "id": "fds",
        "aliases": [
          "FDS",
          "fds"
        ],
        "extensions": [
          ".f",
          ".r",
          ".h"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "fds",
        "scopeName": "source.f",
        "path": "./syntaxes/fds.tmLanguage.json"
      }
    ],
    "themes": [
      {
        "label": "fds-theme",
        "uiTheme": "vs-dark",
        "path": "./themes/fds-theme-color-theme.json"
      }
    ]
  }
}
